{
  "name": "api-node-test",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "local": "nodemon --exec babel-node src/index.js",
    "build": "babel ./src -d dist --presets latest-node --ignore node_modules",
    "lint": "eslint ./src",
    "test": "jest --config ./jest.config.json",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@types/jest": "^26.0.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "nodemon": "^1.18.9"
  },
  "engines": {
    "node": ">=10"
  },
  "resolutions": {
    "eslint": "4.19.1",
    "eslint-plugin-jsx-a11y": "6.0.3"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb/base",
      "plugin:import/errors"
